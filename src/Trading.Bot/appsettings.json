{
  "Serilog": {
      "Using":  [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
      "MinimumLevel": {
          "Default": "Information",
          "Override": {
              "Microsoft": "Warning"
          }
      },
      "WriteTo": [
          { "Name": "Console" },
          { "Name": "File", "Args": { "path": "/bot/logs/log.txt" } }
      ]
  },
  "ConnectionStrings": {
      "DefaultConnection": "Data Source=/bot/db/bot.db;"
  },
  "BotSettings":  [
    {
      "Name": "MARKET_MAKER",
      "Setting": {
        "Symbol":  "BTCUSDT"
      }
    }
  ],
  "ApiKeys": {
      "Key": "teset",
      "Secret": "test"
  }
}
